<Window x:Class="WPF_MVVM.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_MVVM"
        xmlns:localv="clr-namespace:WPF_MVVM.Views"
        xmlns:localvm="clr-namespace:WPF_MVVM.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Window.Resources>
        <localvm:ObjRectangle x:Key="r" Top="100" Left="100" Width="200" Height="200"/>
    </Window.Resources>
    <StackPanel>
        <localv:VMap x:Name="map" Height="300">

        </localv:VMap>
        <Button Content="Button" Width="100" Height="24" Click="Button_Click"/>
    </StackPanel>

</Window>
