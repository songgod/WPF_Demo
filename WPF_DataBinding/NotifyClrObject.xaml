<Window x:Class="WPF_DataBinding.NotifyClrObject"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="NotifyClrObject" Height="300" Width="300">
    <StackPanel>
        <TextBox x:Name="tbName" Margin="5" Text="Name"/>
        <TextBox x:Name="tbID" Margin="5" Text="ID"/>
        <TextBox x:Name="tbSkill" Margin="5" Text="Skill"/>
        <Button Content="Button" Click="Button_Click"/>
    </StackPanel>
</Window>
